{
  "name": "cyberdock",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "dev": "concurrently \"npm run serve\" \"npm run backend\"",
    "backend": "cd backend && npm run dev"
  },
  "dependencies": {
    "apexcharts": "^5.3.2",
    "axios": "^1.11.0",
